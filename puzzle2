function traveldown(){
 var walk = 0;
  while(walk < 6)
  {
    down();
  	walk = walk + 1;
  }
}

traveldown();

function travelright(){
  var walk = 0;
  while(walk < 3)
  {
    right();
    walk = walk + 1;
  }
}

travelright();
up(); 
up();

right();
right();

function traveldownwards(){
  var car= 0;
  while (car < 3)
  {
    down();
    car = car + 1;
  }
}
